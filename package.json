{
  "name": "gimme-your-content-punk",
  "version": "1.0.0",
  "description": "Small web to showcase the benefits of font loading strategies",
  "main": "static/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "rollup -c rollup.config.js",
    "subset": "glyphhanger ./static/unoptimized/index.html --subset=*.woff --formats=woff2,woff",
    "serve": "http-server static -p 5000"
  },
  "keywords": [
    "web",
    "font",
    "strategies",
    "content",
    "foit",
    "fout"
  ],
  "author": "Jeremias Menichelli",
  "license": "MIT",
  "dependencies": {
    "fontfaceobserver": "^2.0.13",
    "store-css": "^0.3.0"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "rollup": "^0.66.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0"
  }
}
